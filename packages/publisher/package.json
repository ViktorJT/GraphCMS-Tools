{
  "private": false,
  "name": "@graphcms-tools/publisher",
  "version": "0.0.1",
  "description": "Publish content to GraphCMS",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "build:types": "tsc -p config/tsconfig.types.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "graphCMS",
    "data",
    "export",
    "import",
    "publish",
    "typescript",
    "npm",
    "package",
    "cjs",
    "esm"
  ],
  "author": "Viktor Jensen-Torp",
  "license": "MIT",
  "homepage": "https://github.com/ViktorJT/GraphCMS-Tools#graphcms-tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/ViktorJT/GraphCMS-Tools.git"
  },
  "bugs": {
    "url": "https://github.com/ViktorJT/GraphCMS-Tools/issues",
    "email": "v.jensentorp@gmail.com"
  },
  "contributors": [
    {
      "name": "Viktor Jensen-Torp",
      "email": "v.jensentorp@gmail.com",
      "url": "https://github.com/ViktorJT"
    }
  ],
  "dependencies": {
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "ora": "^6.1.0"
  },
